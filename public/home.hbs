<div class="container">
    <div class="container posts">
        {{#each posts}}
            <div class="post">
                <div class="titulo">
                    <a href="/post/{{id}}" class="link-dark text-decoration-none">{{titulo}}</a>
                </div>
                <div class="info posts">
                    <ul class="list-inline">
                        <li class="list-inline-item"><i class="fas fa-user"></i>{{nome}}</li>
                        <li class="list-inline-item"><i class="fas fa-calendar"></i> {{data}}</li>
                    </ul>
                </div>
            </div>
        {{/each}}
    </div>
</div>