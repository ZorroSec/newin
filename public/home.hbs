<div class="container">
    <div class="container posts">
        {{#each posts}}
            <div class="card mb-3">
                <div class="card-body">
                    <div class="card-title">
                        <a href="/post/{{id}}">{{titulo}}</a>
                    </div>
                    <div class="card-text">
                        {{data}}
                        <a href="/{{nome}}">{{nome}}</a>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
</div>